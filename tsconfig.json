{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "allowJs": true,
    "paths": {
      "@/*": ["/*"],
      "@root/*": ["../*"],
      "@app/*": ["app/*"],
      "@api": ["api"],
      "@entities/*": ["entities/*"],
      "@shared/*": ["shared/*"],
      "@composables": ["composables"],
      "@features/*": ["features/*"],
      "@components/*": ["components/*"],
      "@widgets/*": ["components/lib/*"],
      "@ui/*": ["components/ui/*"],
      "@layouts/*": ["components/containers/*-layout"],
      "@views/*": ["components/containers/*-view"],
      "@utils": ["utils"],
      "@types": ["types"],
      "@styles/*": ["styles/*"]
    }
  }
}
